    <script type="text/javascript">
		$(document).ready(function() {
			var $cycle = false;
			$( ".more" ).click(function() {
				$( ".advance" ).toggleClass('show');
				if(!$cycle){
					$('.cycle').cycle({
						fx: 'scrollLeft',
						pager: '.pager',
						next:   '.prev', 
						prev:   '.next',
						timeout: null
					});
					$('.cycle2').cycle({
						fx: 'scrollLeft',
						pager: '.pager2',
						next:   '.prev2', 
						prev:   '.next2',
						timeout: null
					});
					$cycle = true;
				}
				$( ".each ul li a" ).click(function() {
					$(this).parents("li").toggleClass('selected');
					return false
				});
				return false
			});

    		$('.cycle-places').cycle({
    			fx: 'scrollLeft',
    			pager: '.pager3',
    			next:   '.left', 
    		   	prev:   '.right',
    		   	timeout: null
    		});
    		$(".places").tabs ();
    		$('a.lbc').fancybox({
				padding: 0,
				overlayColor: '#FFF',
				overlayOpacity: '0.7',
				showCloseButton: 'false'
			});
			
			$('#search_all .map .cont > a').click(function(){
				$('#search_all .map .cont > a').removeClass('active');
				$(this).addClass('active');
				return false;
			});
		});
    </script>
