		var shown = false;
		
		$(function(){
			$('#lstng_htl .carousel ul li').hover(function(){
				$('.bigImages .hover').hide();
				$($('a',this).data('tip')).show();
				$('.bigImages').fadeIn('fast');
				shown=true;
			}, function(){
				shown=false;
			});
			
			$('.bigImages').hover(function(){
				shown=true;
			}, function(){
				shown=false;
